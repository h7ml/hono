{
  "name": "hono",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "$npm_execpath run build && vite preview",
    "deploy": "$npm_execpath run build && wrangler deploy",
    "cf-typegen": "wrangler types --env-interface CloudflareBindings",
    "db:migrate:local": "wrangler d1 migrations apply dev --local",
    "db:migrate:remote": "wrangler d1 migrations apply dev --remote"
  },
  "dependencies": {
    "hono": "^4.11.9"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "^1.2.3",
    "@tailwindcss/vite": "^4.1.18",
    "daisyui": "^5.5.18",
    "tailwindcss": "^4.1.18",
    "vite": "^6.3.5",
    "vite-ssr-components": "^0.5.2",
    "wrangler": "^4.17.0"
  }
}
